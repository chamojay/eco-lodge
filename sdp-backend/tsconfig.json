{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./"
  },
  "include": ["./app/**/*.ts", "./prisma/**/*.ts", "./middleware/**/*.ts", "./services/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}